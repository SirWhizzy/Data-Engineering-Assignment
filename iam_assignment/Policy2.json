{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Statement1",
            "Effect": "Allow",
            "Action": [
                "iam:CreateUser",
                "iam:ListUsers"
            ],
            "Resource": "*",
            "Condition": {
                "StringLike": {
                    "iam:UserName": "engineer*"
                }
            }
        }
    ]
}
